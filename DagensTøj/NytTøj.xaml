        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DagensTøj"
        mc:Ignorable="d"
        Title="NytTøj" Height="450" Width="800">
    <Grid>
        <Button Content="Tilbage" HorizontalAlignment="Left" Margin="687,363,0,0" VerticalAlignment="Top" Width="75" Click="btn_Return"/>
